{
  "$schema": "https://raw.githubusercontent.com/microsoft/AL-Go/32e66d69cfd2c1e3eca39e416e27c6aa58de6df3/Actions/.Modules/settings.schema.json",
  "projectName": "Apps (W1)",
  "appFolders": [
    "../../../src/Apps/W1/*/App",
    "../../../src/Apps/W1/*/*/App"
  ],
  "testFolders": [
    "../../../src/Apps/W1/*/Test",
    "../../../src/Apps/W1/*/Test Library",
    "../../../src/Apps/W1/*/Demo Data",
    "../../../src/Apps/W1/*/*/Test",
    "../../../src/Apps/W1/*/*/Test Library",
    "../../../src/Apps/W1/*/*/Demo Data"
  ],
  "installOnlyReferencedApps": false,
  "doNotRunPageScriptingTests": true,
  "doNotRunBcptTests": true,
  "buildModes": [
    "CH", 
    "CZ", 
    "IN", 
    "US"
  ],
  "ConditionalSettings": [
    {
      "buildModes": [
        "CH"
      ],
      "settings": {
        "country": "ch",
        "doNotRunTests": true
      }
    },
    {
      "buildModes": [
        "CZ"
      ],
      "settings": {
        "country": "cz",
        "doNotRunTests": true
      }
    },
    {
      "buildModes": [
        "IN"
      ],
      "settings": {
        "country": "in",
        "doNotRunTests": true
      }
    },
    {
      "buildModes": [
        "US"
      ],
      "settings": {
        "country": "us",
        "doNotRunTests": true
      }
    },
    {
      "buildModes": [
        "Clean"
      ],
      "settings": {
        "doNotRunTests": true,
        "useCompilerFolder": true,
        "doNotPublishApps": true
      }
    },
    {
      "branches": [
        "releases/*.[0-5]"
      ],
      "settings": {
        "buildModes": [
          "Strict"
        ]
      }
    }
  ],
  "useProjectDependencies": true
}
